{
  "expo": {
    "name": "doctor-drone",
    "slug": "doctor-drone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/icon.png",
    "splash": {
      "image": "./public/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF0000"
    },
    "sdkVersion":"39.0.0",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.dr.drone",
      "versionCode": 1,
      "permissions":[],
      "icon":"./public/icon.png",
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./public/favicon.png"
    }
  }
}
